USE test;

CREATE CONTINUOUS CHANNEL TweetsWithSchools(code) PERIOD duration("PT3S")
{
  SELECT t
  FROM Tweets t WHERE area_code = t.area_code  AND current_channel_time(t) > current_datetime()
};

CREATE BROKER brokerA AT "http://127.0.0.1:10100";